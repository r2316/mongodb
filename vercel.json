{
    "version": 2,
    "builds": [
        {
            "src": "backend/index.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/backend/(.*)",
            "dest": "/backend/index.js"
        },
        {
            "src": "/(.*)",
            "dest": "/backend/index.js"
        }
    ]



}